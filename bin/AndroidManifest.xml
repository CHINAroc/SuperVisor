<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gdut.supervisor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="9" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/MyTheme" >
        <activity
            android:name="com.gdut.supervisor.MainActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
                     <!-- SupervisorActivity，三个表单的主界面 -->
           <activity
            android:name=".SupervisorActivity" 
            android:theme="@android:style/Theme.NoTitleBar"
            >
            </activity>
            <!--表单一对应Activity -->
            <activity
            android:name=".FirstItemActivity"
            android:theme="@android:style/Theme.Black.NoTitleBar">        
        </activity>
         <!--表单二对应Activity -->
        <activity
            android:name=".SecondItemActivity"
            android:theme="@android:style/Theme.Black.NoTitleBar">        
        </activity>
         <!--表单三对应Activity -->
        <activity
            android:name=".ThirdItemActivity"
            android:theme="@android:style/Theme.Black.NoTitleBar">        
        </activity>
    </application>


</manifest>
